<div class="row blog xxlarge-top-margin xlarge-bottom-padding">
  <div class="col-xs-10 col-xs-offset-1">
    <% if !@blogs.empty? %>
      <%= render @blogs %>
      <%= will_paginate @blogs %>
    <% else %>
      <div class="large-font text-center">
        We have no later Archives
        <br>
        <%= link_to('<i class="fa fa-long-arrow-left"></i> Head On Back '.html_safe, blogs_path(page: params[:page].to_i-1), class: "previous_page") %>
      </div>
    <% end %>
  </div>
</div>